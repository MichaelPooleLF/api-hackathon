{
  "name": "api-hackathon",
  "version": "1.0.0",
  "description": "A front-end javaScript application to look up events and breweries by city and state.",
  "main": "server/index.js",
  "scripts": {
    "dev": "npm-run-all --silent --parallel dev:*",
    "dev:client": "webpack-dev-server",
    "dev:server": "nodemon server/index.js",
    "build": "webpack --mode=production",
    "start": "NODE_ENV=production node server/index.js"
  },
  "nodemonConfig": {
    "watch": [
      "server/"
    ],
    "ignore": [
      "server/public/"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelPooleLF/api-hackathon.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MichaelPooleLF/api-hackathon/issues"
  },
  "homepage": "https://github.com/MichaelPooleLF/api-hackathon#readme",
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "npm-run-all": "4.1.5",
    "webpack": "5.11.1",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.11.1"
  },
  "dependencies": {
    "eslint": "7.17.0",
    "express": "4.17.1"
  }
}
